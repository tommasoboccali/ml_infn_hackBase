for i in `cat addr_list.txt`
do
   echo Working on machine $i
   scp -o StrictHostKeyChecking=no -i ~/.ssh/id_rsa create_vm.sh root@$i:/root/create_vm.sh
   ssh -o StrictHostKeyChecking=no -i ~/.ssh/id_rsa root@$i  'chmod +x /root/create_vm.sh'
   ssh -o StrictHostKeyChecking=no -i ~/.ssh/id_rsa root@$i  '/root/create_vm.sh'
done
